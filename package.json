{
  "name": "ai-note-assistant",
  "version": "1.0.0",
  "description": "AI-powered cross-platform note-taking application with GitHub sync",
  "author": "miscmo",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "vite --mode electron",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "@milkdown/core": "^7.17.3",
    "@milkdown/ctx": "^7.17.3",
    "@milkdown/plugin-history": "^7.17.3",
    "@milkdown/plugin-listener": "^7.17.3",
    "@milkdown/plugin-prism": "^7.17.3",
    "@milkdown/preset-commonmark": "^7.17.3",
    "@milkdown/preset-gfm": "^7.17.3",
    "@milkdown/prose": "^7.17.3",
    "@milkdown/theme-nord": "^7.17.3",
    "@octokit/rest": "^20.0.2",
    "@tiptap/extension-bubble-menu": "^3.14.0",
    "@tiptap/extension-code-block-lowlight": "^3.14.0",
    "@tiptap/extension-floating-menu": "^3.14.0",
    "@tiptap/extension-highlight": "^3.14.0",
    "@tiptap/extension-image": "^3.14.0",
    "@tiptap/extension-link": "^3.14.0",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/extension-table": "^3.14.0",
    "@tiptap/extension-task-item": "^3.14.0",
    "@tiptap/extension-task-list": "^3.14.0",
    "@tiptap/extension-text-align": "^3.14.0",
    "@tiptap/extension-typography": "^3.14.0",
    "@tiptap/extension-underline": "^3.14.0",
    "@tiptap/pm": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "@tiptap/vue-3": "^3.14.0",
    "@types/prismjs": "^1.26.5",
    "@vueuse/core": "^10.7.0",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "lowlight": "^3.3.0",
    "marked": "^11.1.0",
    "pinia": "^2.1.7",
    "prismjs": "^1.30.0",
    "sql.js": "^1.10.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-vue": "^4.5.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "madge": "^8.0.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^1.8.25"
  },
  "build": {
    "appId": "com.ainote.assistant",
    "productName": "AI Note Assistant",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "icon": "build/icon.png",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Office"
    }
  }
}
